{{#each customselectors}}{{this}},{{/each}}
{{prefix}}{{name}} {
    background-image: url('{{datauri}}');
    background-repeat: no-repeat;
    width:60px;
    height:38px;
}
{{prefix}}{{name}}.xs {
    width:40px;
    height:25px;
}
{{prefix}}{{name}}.sm {
    width:50px;
    height:32px;
}
{{prefix}}{{name}}.md {
    width:60px;
    height:38px;
}
{{prefix}}{{name}}.lg {
    width:70px;
    height:44px;
}
{{prefix}}{{name}}.xl {
    width:90px;
    height:57px;
}
{{prefix}}{{name}}.xxl {
    width:120px;
    height:76px;
}